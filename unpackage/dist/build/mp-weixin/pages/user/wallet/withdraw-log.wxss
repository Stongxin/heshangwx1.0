.log-list{min-height:213rpx;background:#fff;margin-bottom:10rpx;padding-bottom:10rpx}.log-list .head{padding:0 35rpx;height:80rpx;border-bottom:1rpx solid #eee;margin-bottom:20rpx}.log-list .head .title{font-size:28rpx;font-weight:500;color:#333}.log-list .head .num{font-size:28rpx;font-weight:500;color:#7063d2}.log-list .item{padding:0 30rpx 10rpx}.log-list .item .item-icon{color:silver;font-size:36rpx;margin-right:8rpx}.log-list .item .item-title{width:180rpx;font-size:24rpx;font-weight:400;color:silver}.log-list .item .status-text{font-size:24rpx;font-weight:500;color:#05c3a1}.log-list .item .time{font-size:24rpx;font-weight:400;color:silver}