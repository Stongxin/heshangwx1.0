.apply-commission-wrap{background-color:#fff}.apply-commission-wrap .head-box{background:url(https://lysmall.heshangsz.com/imgs/user/sh_leader_apply_head.png) no-repeat;background-size:100% auto;height:370rpx}.apply-form{width:750rpx;background:#fff;border-radius:20rpx;padding:30rpx}.apply-form .agreement{margin-top:20rpx}.apply-form .agreement .agreement-text{font-size:24rpx;font-weight:500;color:#4cb89d}.apply-form .save-btn{width:690rpx;line-height:86rpx;background:linear-gradient(90deg,#2dae9c,#6bc29e);box-shadow:0 7rpx 11rpx 2rpx rgba(61,179,156,.34);border-radius:43rpx;font-weight:500;color:#fff;margin:30rpx 0}.week-modal .week-modal--head{height:100rpx;border-bottom:1rpx solid #e4e7ed}.week-modal .week-modal--head .text-cancel{color:#ccc}.week-modal .week-modal--head .text-green{color:#18b566}.week-modal .week-modal--content .week-btn{margin-right:56rpx;margin-bottom:30rpx}.week-modal .week-modal--content .week-btn:nth-of-type(4n){margin-right:0}.notice-modal{display:flex;flex-direction:column;justify-content:center;align-items:center;width:612rpx;min-height:658rpx;background:#fff;padding:30rpx;border-radius:20rpx}.notice-modal .img-wrap{margin-bottom:50rpx}.notice-modal .img-wrap .notice-img{width:180rpx;height:170rpx}.notice-modal .notice-title{font-size:35rpx;font-weight:700;color:#46351b;margin-bottom:28rpx}.notice-modal .notice-detail{font-size:28rpx;font-weight:400;color:#999;line-height:36rpx;margin-bottom:50rpx}.notice-modal .notice-btn{width:492rpx;height:70rpx;line-height:70rpx;background:linear-gradient(90deg,#2dae9c,#6bc29e);box-shadow:0 7rpx 11rpx 2rpx rgba(61,179,156,.34);border-radius:35rpx;font-size:28rpx;font-weight:500;color:#fff;margin-bottom:10rpx}.notice-modal .back-btn{width:492rpx;height:70rpx;line-height:70rpx;font-size:28rpx;font-weight:500;color:#3ab29c;background:none}